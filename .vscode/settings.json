{
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
      "source.formatDocument",
  },
  "eslint.validate": [
    "typescript"
  ],
    "[typescript]": {
    "esbenp.prettier-vscode"
    },
  "editor.formatOnSave": true,
  "debug.javascript.unmapMissingSources": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
  },
}
