{
  "name": "msagl-js",
  "version": "0.0.15",
  "description": "MSAGL layout in js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "pkgRoot": "dist",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msaglJS/msagl-js"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc --project tsconfig.prod.json",
    "build_all": "rm -rf dist_all && tsc --project tsconfig.json",
    "eslint": "eslint src",
    "prettier": "prettier --write src/**"
  },
  "dependencies": {
    "@esfx/collections": "^1.0.0-pre.19",
    "@esfx/collections-sortedmap": "^1.0.0-pre.19",
    "@types/parse-color": "^1.0.1",
    "linq-to-typescript": "^8.1.0",
    "prettier": "^2.4.1",
    "queue-typescript": "^1.0.1",
    "reliable-random": "^0.0.1",
    "stack-typescript": "^1.0.4",
    "typescript-string-operations": "^1.4.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/collections": "^5.1.2",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-eslint": "^10.1.0",
    "dotparser": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "lint-staged": "10.5.3",
    "parse-color": "^1.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.0",
    "xml-writer": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint",
      "npm run prettier"
    ]
  },
  "eslintIgnore": [
    "preserve/**"
  ]
}
