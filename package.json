{
  "name": "msagl-js",
  "version": "0.0.3",
  "description": "MSAGL layout in js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "pkgRoot": "dist",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msaglJS/msagl-js"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "eslint": "eslint [src/**]",
    "prettier": "prettier --write src/**"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  },
  "dependencies": {
    "@types/graphlib": "2.1.6",
    "@types/lodash": "4.14.157",
    "collections": "^5.1.12",
    "dotparser": "^1.0.0",
    "file-system": "^1.2.2",
    "graphlib": "2.1.8",
    "linq-to-typescript": "^8.0.0",
    "lodash": ">=4.17.21",
    "npm": "^7.6.0",
    "parse-color": "^1.0.0",
    "queue-typescript": "^1.0.1",
    "stack-typescript": "^1.0.4",
    "tsc": "1.20150623.0",
    "typescript": "^3.9.6",
    "typescript-string-operations": "^1.4.1",
    "xml-writer": "^1.7.0"
  },
  "devDependencies": {
    "@types/collections": "^5.1.0",
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "7.15.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint",
      "npm run prettier"
    ]
  },
  "eslintIgnore": [
    "preserve/**"
  ]
}